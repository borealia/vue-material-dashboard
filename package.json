{
  "name": "vue-material-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve",
    "deploy": "npm run build && sudo gh-pages -d dist",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "chartist": "^0.11.0",
    "google-maps": "^3.2.1",
    "vue": "^2.5.13",
    "vue-analytics": "^5.10.3",
    "vue-clickaway": "^2.2.2",
    "vue-github-buttons": "^2.0.5",
    "vue-material": "^1.0.0-beta-8",
    "vue-router": "^3.0.1",
    "vue-social-sharing": "^2.3.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-standard": "^3.0.0-beta.6",
    "babel-plugin-component": "^1.1.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cheerio": "^1.0.0-rc.2",
    "es6-promise": "^4.2.4",
    "webpack": "^4.6.0",
    "lint-staged": "^6.0.0",
    "markdown-it-container": "^2.0.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "vue-template-compiler": "^2.5.13",
    "vuepress": "^0.8.4",
    "webpack": "^4.6.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "author": "dragosct <dragosct10@gmail.com>",
  "description": "Vue Material Dashboard",
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
